<div class="card">
    <div class="card-header bg-dark text-light">Level 1</div>
    <div class="card-body">

        <ol>
            <li>...all womens' leagues</li>
            <li>...all leagues where sport is any type of hockey</li>
            <li>...all leagues where sport is something OTHER THAN football</li>
            <li>...all leagues that call themselves "conferences"</li>
            <li>...all leagues in the Atlantic region</li>
            <li>...all teams based in Dallas</li>
            <li>...all teams named the Raptors</li>
            <li>...all teams whose location includes "City"</li>
            <li>...all teams whose names begin with "T"</li>
            <li>...all teams, ordered alphabetically by location</li>
            <li>...all teams, ordered by team name in reverse alphabetical order</li>
            <li>...every player with last name "Cooper"</li>
            <li>...every player with first name "Joshua"</li>
            <li>...every player with last name "Cooper" EXCEPT those with "Joshua" as the first name</li>
            <li>...all players with first name "Alexander" OR first name "Wyatt"</li>
        </ol>
    </div>
    <div>
        <ol>
            <li>@foreach(var item in ViewBag.WomensLeagues){@item.Name <br>}</li>
            <li>@foreach(var item in ViewBag.HockeyLeagues){@item.Name <br>}</li>
            <li>@foreach(var item in ViewBag.NotFootball){@item.Name <br>}</li>
            <li>@foreach(var item in ViewBag.Conference){@item.Name <br>}</li>
            <li>@foreach(var item in ViewBag.Atlantic){@item.Name <br>}</li>
            <li>@foreach(var item in ViewBag.Dallas){
                <p>@item.TeamName @item.Location</p>
                }</li>
            <li>@foreach(var item in ViewBag.Raptors){
                <p>@item.TeamName @item.Location</p>
                }</li>

            <li>@foreach(var item in ViewBag.City){@item.TeamName <br>}</li>
            <li>@foreach(var item in ViewBag.T){@item.TeamName <br>}</li>
            <li>@foreach(var item in ViewBag.Alpha){@item.TeamName <br>}</li>
            <li>@foreach(var item in ViewBag.Zulu){@item.TeamName<br>}</li>
            <li>@foreach(var item in ViewBag.Cooper){<p>@item.FirstName @item.LastName</p>}</li>

            <li>@foreach(var item in ViewBag.NotJosh){<p>@item.FirstName @item.LastName</p>}</li>
            
            
            <li>@foreach(var item in ViewBag.WyattOrAlex){<p>@item.FirstName @item.LastName</p>}</li>

        </ol>

    </div>
</div>